{
  "name": "POC Feedback",
  "author": {
    "name": "Joe McCarron",
    "email": "joe@zendesk.com",
    "url": "http://joem.cc"
  },
  "defaultLocale": "en",
  "private": true,
  "location": "ticket_sidebar",
