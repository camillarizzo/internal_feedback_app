    },
    {
      "name":"Fail_notice",
      "type":"text"
    },
    {
      "name":"Label1",
      "type":"text",
      "required":true
    },
    {
      "name":"Tag1",
      "type":"text",
      "required":true
    },
    {
      "name":"Label2",
      "type":"text",
      "required":true
    },
    {
      "name":"Tag2",
      "type":"text",
      "required":true
    },
    {
      "name":"Rating_tag_prefix",
      "type":"text"
    }
